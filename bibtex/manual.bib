doi = {10.1016/j.ast.2019.04.014},
file = {:home/murray/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/de Angelis - - Swing angle estimation for multicopter slung load applications(3).pdf:pdf;:home/murray/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/de Angelis - - Swing angle estimation for multicopter slung load applications(4).pdf:pdf},
issn = {12709638},
