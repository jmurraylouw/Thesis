file = {:home/murray/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Saderla, Kim, Ghosh - 2018 - Online system identification of mini cropped delta UAVs using flight test methods(2).pdf:pdf},
issn = {12709638},
journal = {Aerospace Science and Technology},
keywords = {Delta wing,Least square,Online system identification,Real time parameter estimation,Recursive Fourier transform,UAV},
pages = {337--353},
publisher = {Elsevier Masson SAS},
title = {{Online system identification of mini cropped delta UAVs using flight test methods}},
url = {https://doi.org/10.1016/j.ast.2018.07.008},
volume = {80},
year = {2018}
}
