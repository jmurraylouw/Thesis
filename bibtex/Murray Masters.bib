file = {:home/murray/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Nonomura, Shibata, Takaki - 2019 - Extended-Kalman-filter-based dynamic mode decomposition for simultaneous system identification and(2).pdf:pdf},
issn = {19326203},
journal = {PLoS ONE},
number = {2},
